  </SimulationDescriptor>

  <SimulationDescriptor Name="ClassPopulationPlanetModifiersTraitPrimaryIDS" Type="ClassPopulationPlanet">
        <Modifier       TargetProperty="BonusPopulationIndustry"  Operation="Addition"    Value="5" Path="ClassPopulation"/>
        <Modifier       TargetProperty="BonusPopulationDust"      Operation="Addition"    Value="2" Path="ClassPopulation"/>
        <Modifier       TargetProperty="BonusPopulationScience"   Operation="Addition"    Value="5" Path="ClassPopulation"/>
    </SimulationDescriptor>

</Datatable>