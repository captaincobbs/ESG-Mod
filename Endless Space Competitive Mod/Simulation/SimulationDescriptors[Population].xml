  <SimulationDescriptor Name="ClassPopulationStarSystem" Type="ClassPopulation">
    <Property Name="PopulationTypeCount"          BaseValue="1"/>
    <Property Name="PopulationCountSuperiorToZero"			  BaseValue="0" MinValue="0" MaxValue="1"/>

    <Property Name="PopulationScoreStock"         BaseValue="0"                 MinValue="Negative"     IsSealed="true"    IsSerializable="true"/>
    <Property Name="NetPopulationScore"           BaseValue="0"                 MinValue="-100"    />

    <Property Name="PoliticsTrend01"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend02"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend03"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend04"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend05"              BaseValue="0" IsSealed="true"/>
    <Property Name="PoliticsTrend06"              BaseValue="0" IsSealed="true"/>

    <!-- MinValue is a safe guard. -->
    <Property Name="PoliticsScoreDecayStep"       BaseValue="1"  MinValue="0.1"/>

    <Property Name="PopulationGrowthMalus"        BaseValue="1" MinValue="Negative"/>
    <Property Name="NetPopulationScoreModifier"   BaseValue="1" MinValue="Negative"/>

    <Property Name="CannotMigrate"               MinValue="0" MaxValue="1" />

    <Property Name="ShouldPopulationGrow"        BaseValue="1" MinValue="-1" MaxValue="1"/>
    <Property Name="ShouldPopulationIncrease"    BaseValue="1" MinValue="-1" MaxValue="1"/>

    <!--Bonus Collection Properties-->
    <Property Name="SystemFIDSIPercentBonusFromCollection"/>
    <Property Name="SystemGrowthBonusFromCollection"/>
    <Property Name="SystemProductionBonusFromCollection"/>
    <Property Name="SystemMoneyBonusFromCollection"/>
    <Property Name="SystemResearchBonusFromCollection"/>
    <Property Name="SystemResearchZvaliBonusFromCollection01"/>
    <Property Name="SystemResearchZvaliBonusFromCollection02"/>
    <Property Name="SystemEmpirePointBonusFromCollection"/>
    <Property Name="HappinessBonusFromCollection"/>
    <Property Name="ShipsConstructionProductionBonusFromCollection"/>
    <Property Name="BuildingConstructionBonusFromCollection"/>
    <Property Name="BuildingBuyoutReductionBonusFromCollection"/>
    <Property Name="MaximumSystemManpowerBonusFromCollection"/>
    <Property Name="FlatMaximumSystemManpowerBonusFromCollection"/>
    <Property Name="NetSystemManpowerBonusFromCollection"/>
    <Property Name="NetSystemManpowerPercentBonusFromCollection" BaseValue="0"/>
    <Property Name="NetSystemManpowerBonusFromCollectionHissho"/>
    <Property Name="GroundBattleBombardmentAttackerDamagesBonusFromCollection"/>
    <Property Name="LuxuryBonusFromCollection"/>
    <Property Name="VisionRangeBonusFromCollection"/>

    <Property Name="SystemGrowthBonusFromCollectionHissho"/>
    <Property Name="DockedFleetsHealthRegenerationRatioBonusFromCollection"/>

    <BinaryModifier TargetProperty="PopulationType"	Operation="Addition"   Left="1" BinaryOperation="Multiplication" Right="$(PopulationCountSuperiorToZero)" Path="./ClassStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PopulationCountSuperiorToZero" Operation="Addition"   Value="$(PopulationCount)"      TooltipHidden="true"/>

    <!-- Score evolution -->
    <BinaryModifier TargetProperty="NetPopulationScore" Operation="Addition" Left="50"  BinaryOperation="Multiplication" Right="$(NetPopulationScoreModifier)" TooltipHidden="true"/>
    <Modifier TargetProperty="NetPopulationScore"          Operation="Multiplication"                	Value="$(PopulationGrowthMalus)"           		TooltipHidden="true"/>
    <Modifier TargetProperty="NetPopulationScore"          Operation="Multiplication"                	Value="$(ShouldPopulationGrow)"                	TooltipHidden="true"   Priority="2"/>

