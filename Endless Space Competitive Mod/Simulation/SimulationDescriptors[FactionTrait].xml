    <Modifier TargetProperty="MinorTraitsAssimilated" Operation="Addition" Value="1" Path="ClassEmpire" TooltipHidden="true"/>
  </SimulationDescriptor>

  <!--Galvrans-->
  <SimulationDescriptor Name="FactionTraitMinorMinesOverMatter" Type="FactionTraitMinor">
    <BinaryModifier TargetProperty="SystemResearch" Operation="Addition" Left="30" BinaryOperation="Multiplication" Right="$(OwnedStrategicDeposits)" SearchValueFromPath="true" Path="ClassEmpire/ClassColonizedStarSystem" />
    <Modifier TargetProperty="MinorTraitsAssimilated" Operation="Addition" Value="1" Path="ClassEmpire" TooltipHidden="true"/>
  </SimulationDescriptor>

  <!--Sowers-->
  <SimulationDescriptor Name="FactionTraitMinorSoilHealers" Type="FactionTraitMinor">
    <BinaryModifier TargetProperty="SystemGrowth" Operation="Addition" Left="15" BinaryOperation="Multiplication" Right="$(OwnedMeagerPlanets)" SearchValueFromPath="true" Path="ClassEmpire/ClassColonizedStarSystem"/>
    <BinaryModifier TargetProperty="PlanetRawFIDSI" Operation="Addition" Left="1" BinaryOperation="Multiplication" Right="$(ReducedAnomalyCount)" SearchValueFromPath="true" Path="ClassEmpire/ClassColonizedStarSystem/ClassColonizedPlanet" TooltipHidden="true" />
    <Modifier TargetProperty="PlanetRawFIDSI" Operation="Addition" Value="1" Path="ClassPlanet,PlanetWithReducedAnomaly"/>
    <Modifier TargetProperty="MinorTraitsAssimilated" Operation="Addition" Value="1" Path="ClassEmpire" TooltipHidden="true"/>
  </SimulationDescriptor>

  <!--Greenman-->
  <SimulationDescriptor Name="FactionTraitMinorProfitableExploitations" Type="FactionTraitMinor">
    <BinaryModifier TargetProperty="SystemMoney" Operation="Addition" Left="30" BinaryOperation="Multiplication" Right="$(OwnedStrategicDeposits)" Path="ClassEmpire/ClassColonizedStarSystem" SearchValueFromPath="true"/>
    <Modifier TargetProperty="MinorTraitsAssimilated" Operation="Addition" Value="1" Path="ClassEmpire" TooltipHidden="true"/>
  </SimulationDescriptor>

  <!--Yussho-->
  <SimulationDescriptor Name="FactionTraitMinorDamage" Type="FactionTraitMinor">
