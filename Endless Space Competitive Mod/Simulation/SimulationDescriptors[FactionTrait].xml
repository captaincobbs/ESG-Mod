    <Modifier TargetProperty="MinorTraitsAssimilated" Operation="Addition" Value="1" Path="ClassEmpire" TooltipHidden="true"/>
  </SimulationDescriptor>

  <!--Eyder-->
  <SimulationDescriptor Name="FactionTraitMinorHoarders02" Type="FactionTraitMinor">
    <BinaryModifier TargetProperty="EmpireMoney" Operation="Addition" Left="80" BinaryOperation="Multiplication" Right="$(HeroCount)" Path="ClassEmpire" SearchValueFromPath="true"/>
    <Modifier TargetProperty="MinorTraitsAssimilated" Operation="Addition" Value="1" Path="ClassEmpire" TooltipHidden="true"/>
  </SimulationDescriptor>

  <!--Basryxo-->
  <SimulationDescriptor Name="FactionTraitMinorKeepItCoolMan" Type="FactionTraitMinor">
