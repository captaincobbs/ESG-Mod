  <!-- Population Boost Cost -->
  <SimulationDescriptor Name="FactionTraitIncreasedBoostCost1" Type="FactionTrait">
    <Modifier TargetProperty="PopulationBoostCostMultiplier" Operation="Percent" Value="0.5" Path="ClassEmpire" TooltipHidden="false"/>
  </SimulationDescriptor>

  <SimulationDescriptor Name="FactionTraitIncreasedBoostCost2" Type="FactionTrait">
    <Modifier TargetProperty="PopulationBoostCostMultiplier" Operation="Percent" Value="1" Path="ClassEmpire" TooltipHidden="false"/>
