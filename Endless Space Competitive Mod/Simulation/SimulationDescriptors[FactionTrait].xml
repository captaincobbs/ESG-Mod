    <Modifier TargetProperty="LawUpkeep"             Operation="Percent" Value="1"   Path="ClassEmpire" TooltipOverride="%EmpireImprovementLawUpkeepIncrease"/>

    <!--Ark essence cost rework (currently busted)-->
    <Modifier TargetProperty="MaximumEmpireLifeforceStock" Operation="Addition" Value="400"     Path="ClassEmpire,AffinityGameplayVampirilis" TooltipHidden="true"/>
