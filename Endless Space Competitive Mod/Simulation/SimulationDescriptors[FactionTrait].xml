    <BinaryModifier TargetProperty="SystemEmpirePoint" Operation="Addition" Left="20" BinaryOperation="Multiplication" Right="$(AllianceCount)" Path="ClassEmpire/ClassColonizedStarSystem" SearchValueFromPath="true"/>
    <Modifier TargetProperty="MinorTraitsAssimilated" Operation="Addition" Value="1" Path="ClassEmpire" TooltipHidden="true"/>
  </SimulationDescriptor>

  <!-- -1 Movement Riftborn -->
  <SimulationDescriptor Name="FactionTraitSlowTraveler1" Type="FactionTrait">
    <Modifier TargetProperty="MaximumMovement" Operation="Addition" Value="-1" Path="ClassEmpire/ClassGarrison/ClassShip"/>
    <Modifier TargetProperty="MaximumMovement" Operation="Addition" Value="-1" Path="ClassEmpire/ClassColonizedStarSystem/ClassGarrison/ClassShip" TooltipHidden="true"/>
    <Modifier TargetProperty="MaximumMovement" Operation="Addition" Value="1" Path="ClassEmpire/ClassGarrisonFleet/ClassShip,AutomatedShip" TooltipHidden="true"/>

    <Modifier TargetProperty="FreeMovementSpeed" Operation="Multiplication" Value="0.9" Path="ClassEmpire/ClassGarrison/ClassShip"/>
  </SimulationDescriptor>

  <!-- -2 Movement -->
  <SimulationDescriptor Name="FactionTraitSlowTraveler2" Type="FactionTrait">
    <Modifier TargetProperty="MaximumMovement" Operation="Addition" Value="-2" Path="ClassEmpire/ClassGarrison/ClassShip"/>
    <Modifier TargetProperty="MaximumMovement" Operation="Addition" Value="-2" Path="ClassEmpire/ClassColonizedStarSystem/ClassGarrison/ClassShip" TooltipHidden="true"/>
    <Modifier TargetProperty="MaximumMovement" Operation="Addition" Value="2" Path="ClassEmpire/ClassGarrisonFleet/ClassShip,AutomatedShip" TooltipHidden="true"/>

    <Modifier TargetProperty="FreeMovementSpeed" Operation="Multiplication" Value="0.8" Path="ClassEmpire/ClassGarrison/ClassShip"/>
  </SimulationDescriptor>

  <!-- Price of Perfection -->
  <SimulationDescriptor Name="FactionTraitPriceOfPerfection1" Type="FactionTrait">
    <Modifier TargetProperty="ShipProductionCostReduction" Operation="Addition" Value="0.05" Path="ClassEmpire/ClassColonizedStarSystem" Priority="-5"/>
    <Modifier TargetProperty="ColonyShipProductionCostReduction" Operation="Addition" Value="0.10" Path="ClassEmpire/ClassColonizedStarSystem" Priority="-5" TooltipHidden="true"/>

    <Modifier TargetProperty="Damage" Operation="Percent" Value="0.05" Path="ClassEmpire/ClassGarrison/ClassShip/ClassSection/ClassModule,ClassModuleWeapon"/>
