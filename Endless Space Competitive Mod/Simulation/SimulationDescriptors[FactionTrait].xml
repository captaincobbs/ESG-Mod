    <Modifier TargetProperty="AssignmentCooldownDecrementPerTurn" Operation="Percent" Value="0.5" Path="ClassEmpire/ClassColonizedStarSystem/ClassHero" TooltipHidden="true"/>
    <Modifier TargetProperty="AssignmentCooldownDecrementPerTurn" Operation="Percent" Value="0.5" Path="ClassEmpire/ClassHero" TooltipHidden="true"/>
  </SimulationDescriptor>

  <SimulationDescriptor Name="FactionTraitScienceReduction" Type="FactionTrait">
      <Modifier TargetProperty="DefaultPlanetScience"     Operation="Force"       Value="0"   Path="FAKEFORGUI/ClassColonizedPlanet"    TooltipHidden="false"/>
      <Modifier TargetProperty="DefaultPlanetScienceMalusFactor"     Operation="Force"       Value="1"   Path="ClassEmpire/ClassColonizedStarSystem/ClassColonizedPlanet"    TooltipHidden="true"/>
      <Modifier TargetProperty="CanUseRawPlanetScience" Operation="Force" Value="0" Path="ClassEmpire" TooltipHidden="true"/>
      <Modifier TargetProperty="CannotUseRawPlanetScience" Operation="Force" Value="1" Path="ClassEmpire" TooltipHidden="true"/>
      <Modifier TargetProperty="CanUseRawPlanetScience" Operation="Force" Value="0" Path="ClassEmpire/ClassColonizedStarSystem" TooltipHidden="true"/>
      <Modifier TargetProperty="CannotUseRawPlanetScience" Operation="Force" Value="1" Path="ClassEmpire/ClassColonizedStarSystem" TooltipHidden="true"/>
      <BinaryModifier TargetProperty="NetSystemResearch" Operation="Addition" Left="10" BinaryOperation="Multiplication" Right="$(SystemResearchRelics)" Path="ClassEmpire/ClassColonizedStarSystem" SearchValueFromPath="true"/>
  </SimulationDescriptor>

  <!-- Academy -->
  <SimulationDescriptor Name="FactionTraitAcademyBonuses" Type="FactionTrait">
    <Modifier TargetProperty="PlanetRawDust" Operation="Addition" Value="5" Path="ClassEmpire/ClassColonizedStarSystem/ClassColonizedPlanet" TooltipHidden="true" />
    <Modifier TargetProperty="PlanetRawFood" Operation="Addition" Value="5" Path="ClassEmpire/ClassColonizedStarSystem/ClassColonizedPlanet" TooltipHidden="true" />
    <Modifier TargetProperty="PlanetRawFood" Operation="Multiplication" Value="2" Path="ClassEmpire/ClassColonizedStarSystem/ClassColonizedPlanet" TooltipHidden="true" />
    <BinaryModifier TargetProperty="MaximumSystemManpower" Operation="Addition" Left="$(Population)" BinaryOperation="Multiplication" Right="25" Path="ClassEmpire/ClassColonizedStarSystem" SearchValueFromPath="true" TooltipHidden="true" />
    <BinaryModifier TargetProperty="GroundBattleBombardmentDefenderDamages" Operation="Addition" Left="$(Population)" BinaryOperation="Multiplication" Right="10" Path="ClassEmpire/ClassColonizedStarSystem" SearchValueFromPath="true" TooltipHidden="true" />
    <Modifier TargetProperty="RawMaximumPopulation" Operation="Force" Value="14" Path="ClassEmpire/ClassColonizedStarSystem/ClassColonizedPlanet" TooltipHidden="true" />
