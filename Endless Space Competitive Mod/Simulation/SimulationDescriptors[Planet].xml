<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/Amplitude.Unity.Simulation.SimulationDescriptor.xsd">
  <SimulationDescriptor Name="ClassPlanet" Type="Class">

    <!-- ########################################################## -->
    <!-- ######################## PROPERTY ######################## -->
    <!-- ########################################################## -->
    <Property Name="OwnedStrategicDeposits"                           BaseValue="0" Composition="Sum" />
    <Property Name="OwnedLuxuryDeposits"                           BaseValue="0" Composition="Sum" />
    <Property Name="OwnedAnomalies"/>
    <Property Name="OwnedMoons"/>
    <Property Name="BoleanOwnedStrategicDeposit" BaseValue="0"  MaxValue="1"/>
    <Property Name="BoleanOwnedLuxuryDeposit" BaseValue="0"  MaxValue="1"/>

    <Property Name="PlanetHasLuxury"        BaseValue="0"   MaxValue="1"/>
    <Property Name="PlanetHasStrategic"        BaseValue="0"   MaxValue="1"/>
    <Property Name="PlanetHasAnomaly"        BaseValue="0"   MaxValue="1"/>
    <Property Name="Population"             BaseValue="0"/>
    <Property Name="PopulationTypeCount"    BaseValue="0"/>
    <Property Name="MajorPopulationCount"	BaseValue="0"  />
    <Property Name="NonMajorPopulationCount" BaseValue="0"  />
    <Property Name="PlanetCount"            BaseValue="1"/>

    <!-- Defined by size, type -->
    <Property Name="RawMaximumPopulation"   BaseValue="0" MinValue="0" MaxValue="14" RoundingFunction="Floor"/>
    <!-- Raw + improvements & stuff -->
    <Property Name="BaseMaximumPopulation"  BaseValue="0" MaxValue="14" RoundingFunction="Floor"/>
    <!-- Default = Base. Can be overriden by motherships for example -->
    <Property Name="MaximumPopulation"      BaseValue="0" MaxValue="14" RoundingFunction="Floor"/>
    <Property Name="ColonizedModifier"      BaseValue="0"/>

    <Property Name="AnomalyCount" BaseValue="0"/>
    <Property Name="ReducedAnomalyCount" BaseValue="0"/>

    <!-- Planet's raw resources -->
    <Property Name="PlanetInitialFIDSPenalty" BaseValue="0" MinValue="Negative"/>

    <Property Name="PlanetInitialFood" MinValue="Negative"/>
    <Property Name="PlanetInitialIndustry" MinValue="Negative"/>
    <Property Name="PlanetInitialDust" MinValue="Negative"/>
    <Property Name="PlanetInitialScience" MinValue="Negative"/>
    <Property Name="PlanetInitialPrestige" MinValue="Negative"/>


    <Property Name="PlanetRawFood" MinValue="Negative"/>
    <Property Name="PlanetRawIndustry" MinValue="Negative"/>
    <Property Name="PlanetRawDust" MinValue="Negative"/>
    <Property Name="PlanetRawScience" MinValue="Negative"/>
    <Property Name="PlanetRawPrestige" MinValue="Negative"/>
    <Property Name="PlanetRawFIDSI" MinValue="Negative"/>
    <Property Name="PlanetRawFIDS" MinValue="Negative"/>

    <Property Name="PlanetDepletionPoints" IsSealed="true"/>
    <Property Name="PlanetDepletionPerTurn" MinValue="Negative"/>
    <Property Name="PlanetDepletionMax" BaseValue="0"/>
    <Property Name="PlanetDepletionThreshold" />
    <Property Name="PlanetDepletionPerTurnFromFleets"       IsSealed="true" MinValue="Negative"/>
    <Property Name="PlanetDepletionPerTurnFromMiningProbe"  IsSealed="true" MinValue="Negative"/>
    <Property Name="PlanetDepletionMultiplier" BaseValue="0" MinValue="Negative"/>


    <Property Name="Strategic1"  />
    <Property Name="Strategic2"  />
    <Property Name="Strategic3"  />
    <Property Name="Strategic4"  />
    <Property Name="Strategic5"  />
    <Property Name="Strategic6"  />

    <Property Name="Luxury1"     />
    <Property Name="Luxury2"     />
    <Property Name="Luxury3"     />
    <Property Name="Luxury4"     />
    <Property Name="Luxury5"     />
    <Property Name="Luxury6"     />
    <Property Name="Luxury7"     />
    <Property Name="Luxury8"     />
    <Property Name="Luxury9"     />
    <Property Name="Luxury10"    />
    <Property Name="Luxury11"    />
    <Property Name="Luxury12"    />
    <Property Name="Luxury13"    />
    <Property Name="Luxury14"    />
    <Property Name="Luxury15"    />
    <Property Name="Luxury16"    />
    <Property Name="Luxury17"    />
    <Property Name="Luxury18"    />
    <Property Name="Luxury19"    />
    <Property Name="Luxury20"    />
    <Property Name="Luxury21"    />
    <Property Name="Luxury22"    />
    <Property Name="Luxury23"    />
    <Property Name="Luxury24"    />

    <!-- Planet happiness value per pop -->
    <Property Name="Happiness"        BaseValue="0" MinValue="Negative"/>
    <!-- Planet base happiness value only due to deposits/anomalies/etc. (mainly used for AI) -->
    <Property Name="HappinessRaw"     BaseValue="0" MinValue="Negative"/>

    <Property Name="ShouldForceRawFood" BaseValue="0" MaxValue="1"/>
    <Property Name="ApplyShouldForceRawFood" BaseValue="0" MaxValue="1"/>

    <Property Name="ShouldForceRawScience" BaseValue="0" MaxValue="1"/>
    <Property Name="ApplyShouldForceRawScience" BaseValue="0" MaxValue="1"/>

    <Property Name="AssignedHeroLevel" BaseValue="0" MinValue="0" />

    <Property Name="CuriositiesLevel1Count" 	BaseValue="0" IsSealed="true"/>
    <Property Name="CuriositiesLevel2Count" 	BaseValue="0" IsSealed="true"/>
    <Property Name="CuriositiesLevel3Count" 	BaseValue="0" IsSealed="true"/>
    <Property Name="CuriositiesLevel4Count" 	BaseValue="0" IsSealed="true"/>

    <Property Name="Guardians" 	BaseValue="0" MinValue="Negative" IsSerializable="true"/>

    <Property Name="PlanetIsDestroyed" 	BaseValue="0" MinValue="0" MaxValue="1"/>

    <Property Name="PlanetInitialFIDSPenalty" BaseValue="0"/>

    <!-- ########################################################## -->
    <!-- ######################## MODIFIER ######################## -->
    <!-- ########################################################## -->

    <Modifier TargetProperty="PlanetInitialFood"        Operation="Percent"    Value="$(PlanetInitialFIDSPenalty)"  TooltipHidden="true"  />
    <Modifier TargetProperty="PlanetInitialIndustry"    Operation="Percent"    Value="$(PlanetInitialFIDSPenalty)"  TooltipHidden="true"  />
    <Modifier TargetProperty="PlanetInitialDust"        Operation="Percent"    Value="$(PlanetInitialFIDSPenalty)"  TooltipHidden="true"  />
    <Modifier TargetProperty="PlanetInitialScience"     Operation="Percent"    Value="$(PlanetInitialFIDSPenalty)"  TooltipHidden="true"  />
    <Modifier TargetProperty="PlanetInitialPrestige"    Operation="Percent"    Value="$(PlanetInitialFIDSPenalty)"  TooltipHidden="true"    />




    <!-- Population max -->
    <Modifier TargetProperty="PlanetRawFood"        Operation="Addition"    Value="$(PlanetInitialFood)"        Priority="-5"   TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawIndustry"    Operation="Addition"    Value="$(PlanetInitialIndustry)"    Priority="-5"   TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawDust"        Operation="Addition"    Value="$(PlanetInitialDust)"        Priority="-5"   TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawScience"     Operation="Addition"    Value="$(PlanetInitialScience)"     Priority="-5"   TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawPrestige"    Operation="Addition"    Value="$(PlanetInitialPrestige)"    Priority="-5"   TooltipHidden="true"/>


    <!-- FOR OUTPOSTS -->
    <Modifier TargetProperty="AdditionalFood"        Operation="Addition"    Value="$(PlanetRawFood)"        Priority="-5"   TooltipHidden="true"   Path="../ClassColonizedStarSystem,ColonizedStarSystemStateOutpost"/>
    <Modifier TargetProperty="AdditionalIndustry"    Operation="Addition"    Value="$(PlanetRawIndustry)"    Priority="-5"   TooltipHidden="true"   Path="../ClassColonizedStarSystem,ColonizedStarSystemStateOutpost"/>
    <Modifier TargetProperty="AdditionalDust"        Operation="Addition"    Value="$(PlanetRawDust)"        Priority="-5"   TooltipHidden="true"   Path="../ClassColonizedStarSystem,ColonizedStarSystemStateOutpost"/>
    <Modifier TargetProperty="AdditionalScience"     Operation="Addition"    Value="$(PlanetRawScience)"     Priority="-5"   TooltipHidden="true"   Path="../ClassColonizedStarSystem,ColonizedStarSystemStateOutpost"/>
    <Modifier TargetProperty="AdditionalPrestige"    Operation="Addition"    Value="$(PlanetRawPrestige)"    Priority="-5"   TooltipHidden="true"   Path="../ClassColonizedStarSystem,ColonizedStarSystemStateOutpost"/>


    <Modifier TargetProperty="BaseMaximumPopulation"    Operation="Addition"    Value="$(RawMaximumPopulation)" TooltipHidden="true"/>

    <Modifier TargetProperty="MaximumPopulation"        Operation="Addition"       Value="$(RawMaximumPopulation)"      Priority="0" Path="ClassPlanet" TooltipHidden="true"/>

    <!-- FIDSI -->
    <Modifier TargetProperty="PlanetRawFood"            Operation="Addition"    Value="$(PlanetRawFIDSI)" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawIndustry"        Operation="Addition"    Value="$(PlanetRawFIDSI)" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawDust"            Operation="Addition"    Value="$(PlanetRawFIDSI)" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawScience"         Operation="Addition"    Value="$(PlanetRawFIDSI)" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawPrestige"        Operation="Addition"    Value="$(PlanetRawFIDSI)" TooltipHidden="true"/>
    <!-- FIDS -->
    <Modifier TargetProperty="PlanetRawFood"            Operation="Addition"    Value="$(PlanetRawFIDS)" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawIndustry"        Operation="Addition"    Value="$(PlanetRawFIDS)" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawDust"            Operation="Addition"    Value="$(PlanetRawFIDS)" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawScience"         Operation="Addition"    Value="$(PlanetRawFIDS)" TooltipHidden="true"/>

    <!-- INVASION -->
    <!--<Modifier TargetProperty="BaseUnitPower" Operation="Addition" Value="5" Path="./ClassColonizedStarSystem"/>-->
    <BinaryModifier TargetProperty="PlanetDepletionThreshold" Operation="Addition" Left="0.5" BinaryOperation="Multiplication" Right="$(PlanetDepletionMax)" TooltipHidden="true"/>


    <Modifier TargetProperty="PlanetHasAnomaly"            Operation="Addition"    Value="$(AnomalyCount)"  TooltipHidden="true" />
    <Modifier TargetProperty="OwnedAnomalies"              Operation="Addition"    Value="$(AnomalyCount)" TooltipHidden="true" />

    <Modifier TargetProperty="PlanetHasAnomaly"            Operation="Addition"    Value="$(PlanetHasAnomaly)" Path="ClassPlanet/ClassPopulationPlanet" TooltipHidden="true"/>


    <BinaryModifier TargetProperty="ShouldForceRawFood" Operation="Addition" Left="5" BinaryOperation="Subtraction" Right="$(PlanetInitialFood)"           TooltipHidden="true" />
    <Modifier       TargetProperty="ShouldForceRawFood" Operation="Multiplication"    Value="$(ApplyShouldForceRawFood)" TooltipHidden="true"/>
    <BinaryModifier    TargetProperty="PlanetRawFood"            Operation="Multiplication" Left="1" BinaryOperation="Subtraction" Right="$(ShouldForceRawFood)"    Priority="-4"   TooltipHidden="true"/>
    <BinaryModifier     TargetProperty="PlanetRawFood"          Operation="Addition" Left="5" BinaryOperation="Multiplication" Right="$(ShouldForceRawFood)"      Priority="-3"     TooltipHidden="true"/>

    <BinaryModifier TargetProperty="ShouldForceRawScience" Operation="Addition" Left="5" BinaryOperation="Subtraction" Right="$(PlanetInitialScience)"           TooltipHidden="true" />
    <Modifier       TargetProperty="ShouldForceRawScience" Operation="Multiplication"    Value="$(ApplyShouldForceRawScience)"      TooltipHidden="true" />
    <BinaryModifier    TargetProperty="PlanetRawScience"            Operation="Multiplication" Left="1" BinaryOperation="Subtraction" Right="$(ShouldForceRawScience)"    Priority="-4" TooltipHidden="true"/>
    <BinaryModifier     TargetProperty="PlanetRawScience"          Operation="Addition" Left="5" BinaryOperation="Multiplication" Right="$(ShouldForceRawScience)"      Priority="-3"   TooltipHidden="true"/>

    <Modifier TargetProperty="PlanetDepletionPerTurn" Operation="Addition" Value="$(PlanetDepletionPerTurnFromFleets)"/>
    <Modifier TargetProperty="PlanetDepletionPerTurn" Operation="Addition" Value="$(PlanetDepletionPerTurnFromMiningProbe)"/>
    <Modifier TargetProperty="PlanetDepletionPerTurn" Operation="Percent" Value="$(PlanetDepletionMultiplier)"/>

  </SimulationDescriptor>


