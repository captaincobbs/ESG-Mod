    <!-- Holy Proliferation -->
	<StarSystemResourceImprovementDefinition Name="StarSystemImprovementGrowthFromLifeforce" SubCategory="SubCategoryVampirilis" ScoreProvider="StarSystemImprovementBuilt">
		<AIGain RessourceName="LifeForceGain" Inverted="true"/>
		<AIGain RessourceName="FoodGain"/>
		<Tags>NoBuyout</Tags>
		<Cost ResourceName="SystemTurnBased">1</Cost>
		<CustomCost ResourceName="EmpireLifeforce"      Instant="true">250 * Max(1,Property(StockLocation, @ClassExploitedStarSystem, PopulationOnMothership, true)) * Property(StockLocation, @../ClassEmpire,GameSpeedMultiplier)</CustomCost>
		<PathPrerequisite Flags="Prerequisite,Discard">../ClassEmpire,AffinityGameplayVampirilis</PathPrerequisite>
		<InterpreterPrerequisite Flags="Prerequisite,Disable">Property(Context, Population) lt Property(Context, MaximumPopulation)</InterpreterPrerequisite>
		<Resource Name="SystemGrowth">1200 * Property(Empire, GameSpeedMultiplier)</Resource>
	</StarSystemResourceImprovementDefinition>
  
</Datatable>