  <!--Horatio pop transform-->
  <StarSystemPopulationTransformImprovementDefinition   Name="StarSystemImprovementCloneMajorPopulation" SubCategory="SubCategoryPopulation" ScoreProvider="StarSystemImprovementBuilt"
														PopulationSource="Minor" PopulationSourceHeuristic="LowestCount" PopulationDestination="Major" PopulationDestinationHeuristic="LowestCount"  PopulationToTransformCount="1" >
      <AIGain RessourceName="PopulationGain" Inverted="false"></AIGain>  
      <QueuedSimulationDescriptorReference Name="StarSystemImprovementCloneMajorPopulationQueued"/>
        <CustomCost ResourceName="SystemProduction">200</CustomCost>
        <InterpreterPrerequisite Inverted="false" Flags="NotEnoughPopulation,Enqueue">(Property(Context, @ClassColonizedStarSystem, Population) -  Property(Context, @ClassColonizedStarSystem, MajorPopulationCount) - Property(Context, @ClassColonizedStarSystem, OnGoingClone) ) gt 0</InterpreterPrerequisite>
        <PathPrerequisite Inverted="false"   Flags="Prerequisite,Discard">../ClassEmpire,AffinityGameplayHoratio</PathPrerequisite>
        <PathPrerequisite Inverted="false"  Flags="Prerequisite,Disable">ColonizedStarSystemStateColony/ClassColonizedPlanet/PopulationNonMajor, !ClassPopulationPlanetAffinityGuardians</PathPrerequisite>
        <TechnologyPrerequisite Flags="Prerequisite,Discard,Technology">TechnologyCloneMajorPopulation</TechnologyPrerequisite>
        <SimulationDescriptorReference Name="StarSystemImprovementCloneMajorPopulation"/>
    </StarSystemPopulationTransformImprovementDefinition>

  <!--Smoke Guardian as non-UF-->
  <StarSystemPopulationTransformImprovementDefinition   Name="StarSystemImprovementSacrificeGuardian2" SubCategory="SubCategoryPopulation" ScoreProvider="StarSystemImprovementBuilt"
                                                    PopulationSource="AffinityGuardians" IgnoreInvinciblePopulation="true">
    <Tags>NoBuyout</Tags>
    <QueuedSimulationDescriptorReference Name="StarSystemImprovementSacrificeGuardianQueued"/>
    <Cost ResourceName="SystemTurnBased">3</Cost>
    <PathPrerequisite Inverted="false"   Flags="Prerequisite,Discard">../ClassEmpire,!FactionTraitGuardians</PathPrerequisite>
    <PathPrerequisite Inverted="false"   Flags="Prerequisite,Discard">../ClassEmpire,!AffinityGameplayMajorHisshos</PathPrerequisite>
    <PathPrerequisite Inverted="false"   Flags="Prerequisite,Discard">ClassColonizedStarSystem/ClassPopulationStarSystemAffinityGuardians</PathPrerequisite>
    <InterpreterPrerequisite Inverted="false" Flags="NotEnoughPopulation,Enqueue">Property(Context,@'ClassColonizedStarSystem/ClassPopulationStarSystemAffinityGuardians', PopulationCount) - Property(Context, @ClassColonizedStarSystem, OnGoingSacrifice) gt 0</InterpreterPrerequisite>
    <InterpreterPrerequisite Flags="Prerequisite,Discard">Property(Context,@'ClassColonizedStarSystem/ClassPopulationStarSystemAffinityGuardians', PopulationCount) ge 1</InterpreterPrerequisite>
    <SimulationDescriptorReference Name="StarSystemImprovementSacrificeGuardian"/>
    <TemporaryEffect>SacrificeGuardianHappiness</TemporaryEffect>
  </StarSystemPopulationTransformImprovementDefinition>

